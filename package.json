{
  "name": "demo-front-prod",
  "version": "1.0.0",
  "scripts": {
    "serve": "wds -r src --watch",
    "build:v0": "mkdir -p dist && cp -r src/v0 dist",
    "build:v1": "./node_modules/.bin/esbuild --bundle src/v1/index.js --outdir=dist/v1 && cp src/v1/*.html dist/v1/",
    "deploy": "./deploy.sh"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.28",
    "esbuild": "^0.13.14"
  }
}
